services:
  aspappang.server:
    build:
      context: C:\CSharp\basic\aspAppAng
      dockerfile: aspAppAng.Server/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: aspappangserver
    networks:
      default: null
